{
    "hpAddr": "1E5992C9BB8",
    "xAddr": "1E590871FBC",
    // 可接受的伤害的范围
    // 就是当伤害在下面任意一个范围内
    // 脚本便认为刷到了目标
    // 此时就会停止工作，需要再次重启
    "acceptDamageRanges": [
        [ // 冰刃伤害范围，需要根据自己实际情况调整 
            820,
            950
        ],
        [ // 钻石星辰伤害范围，需要根据自己实际情况调整
            1850,
            3000
        ]
        // 不建议测试冰雪飞弹和空气炮技能的伤害
        // 因为这两个技能伤害过于接近，很容易导致脚本误判
    ],
    // 不接受的伤害范围
    // 就是当伤害在下面任意一个范围内
    // 脚本认为当前的焕冬兽不满足
    // 直接会将空涡龙操作至 restX, restY 坐标位置
    "rejectDamageRanges": [
        // 伤害不可能低于这个范围
        [
            1,
            100
        ],
        // 这个范围就是冰雪飞弹和空气炮不可能高于的伤害
        // 注意由于是先判断 acceptDamageRanges，所以这里可以写成这么大的范围
        [
            300,
            3000
        ]
    ],
    // 空涡龙休息位置
    "restX": -6.60,
    "restY": 7.90,
    // 空涡龙测试伤害位置，也就是战斗位置
    "fightX": -6.11,
    "fightY": 7.85,
    // 提醒持续时间，单位：ms
    "reminderDuration": 10000
}